<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<servlet>
		<description></description>
		<display-name>MJsonRequestReceiver</display-name>
		<servlet-name>MJsonRequestReceiver</servlet-name>
		<servlet-class>com.itextos.beacon.http.migration.servlet.MJsonRequestReceiver</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>MQSRequestReceiver</display-name>
		<servlet-name>MQSRequestReceiver</servlet-name>
		<servlet-class>com.itextos.beacon.interfaces.migration.servlet.MQSRequestReceiver</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>InitServlet</display-name>
		<servlet-name>InitServlet</servlet-name>
		<servlet-class>com.itextos.beacon.http.generichttpapi.servlet.InitServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<description></description>
		<display-name>ErrorServlet</display-name>
		<servlet-name>ErrorServlet</servlet-name>
		<servlet-class>com.itextos.beacon.commonlib.apperrorhandler.servlets.ErrorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ErrorServlet</servlet-name>
		<url-pattern>/errorservlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ExceptionServlet</display-name>
		<servlet-name>ExceptionServlet</servlet-name>
		<servlet-class>com.itextos.beacon.commonlib.apperrorhandler.servlets.ExceptionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InitServlet</servlet-name>
		<url-pattern>/InitServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MJsonRequestReceiver</servlet-name>
		<url-pattern>/MJsonRequestReceiver</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MQSRequestReceiver</servlet-name>
		<url-pattern>/send</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ExceptionServlet</servlet-name>
		<url-pattern>/exceptionservlet</url-pattern>
	</servlet-mapping>

	<error-page>
		<location>/errorservlet</location>
	</error-page>
	<error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>/exceptionservlet</location>
	</error-page>
</web-app>